<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="net.opentrends.training.model.impl.DepartmentModelImpl">
		<attributes>
			<id name="departmentid" />
			<basic name="deptName" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="net.opentrends.training.model.impl.MarksModelImpl">
		<attributes>
			<id name="marksid" />
			<basic name="rollNo" />
			<basic name="percentage" />
			<basic name="userId" />
			<basic name="groupId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="net.opentrends.training.model.impl.StudentModelImpl">
		<attributes>
			<id name="studentid" />
			<basic name="name" />
			<basic name="dept_id" />
			<basic name="rollNo" />
			<basic name="userId" />
			<basic name="groupId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="net.opentrends.training.model.impl.DepartmentImpl" name="Department">
		<table name="sample_Department" />
		<attributes />
	</entity>
	<entity class="net.opentrends.training.model.impl.MarksImpl" name="Marks">
		<table name="sample_Marks" />
		<attributes />
	</entity>
	<entity class="net.opentrends.training.model.impl.StudentImpl" name="Student">
		<table name="sample_Student" />
		<attributes />
	</entity>
</entity-mappings>